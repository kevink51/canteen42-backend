# Sample environment variables for Canteen42 backend
# Copy this file to .env and fill in your actual values

# Server configuration
PORT=3000
NODE_ENV=development

# Firebase
FIREBASE_PROJECT_ID=canteen42-e1058
# Add your Firebase service account credentials in production

# Stripe
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# PayPal
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret

# Square
SQUARE_ACCESS_TOKEN=your_square_access_token
SQUARE_LOCATION_ID=your_square_location_id

# Tawk.to
TAWK_TO_PROPERTY_ID=your_tawkto_property_id
TAWK_TO_WIDGET_ID=your_tawkto_widget_id

# Dropshipping suppliers (if enabled)
OBERLO_ENABLED=false
OBERLO_API_KEY=your_oberlo_api_key

SPOCKET_ENABLED=false
SPOCKET_API_KEY=your_spocket_api_key

DSERS_ENABLED=false
DSERS_API_KEY=your_dsers_api_key

CJ_DROPSHIPPING_ENABLED=false
CJ_DROPSHIPPING_API_KEY=your_cj_dropshipping_api_key

ALIBABA_ENABLED=false
ALIBABA_API_KEY=your_alibaba_api_key
